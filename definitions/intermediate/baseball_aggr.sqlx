config {
  type: "table",
  schema: "intermediate",
  assertions: {
    uniqueKey: ["gameId"]
  },
  description: "This table defines something",
  columns: {
    gameId: "A test column",
    gameNumber: "A test column",
    year: "A test column",
    dayNight: "A test column",
    duration_minutes: "A test column",
  }
}

SELECT 
  gameId, 
  gameNumber, 
  year, 
  dayNight,
  duration_minutes
FROM 
  ${ref("stg_schedules")}
WHERE 
  dayNight = 'D' 
  and year >= 2016